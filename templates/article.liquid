{% if article.url contains "news" %}
  {% layout 'theme' %}
  {% section 'article-static-slideshow' %}
  {% section 'static-article' %}
  {% comment %} {% section 'contact-cta' %} {% endcomment %}
  {% section 'static-blog-related' %}
{% elsif article.url contains "whitepapers" %}
  {% section 'article-static-slideshow' %}
  {% section 'static-article' %}
  {% section 'featured-products' %}
  {% section 'static-blog-related-wp' %}
  {% comment %} {% section 'contact-cta' %} {% endcomment %}
{% else %}
  {% section 'article-static-slideshow' %}
  {% section 'static-article' %}
  {% comment %} {% section 'contact-cta' %} {% endcomment %}
{% endif %}
<script type="text/javascript" async>
  $('a[href*="#"]').on("click", function(e) {
    e.preventDefault()
    var target = $(this).attr('href').replace('#','')
    $('html,body').animate({scrollTop:$("a[name='"+ target +"']").offset().top - 220},'slow');
  });
</script>