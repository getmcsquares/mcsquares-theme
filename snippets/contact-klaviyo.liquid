<div class="form-selector-group">
<div>
  <input type="radio" id="home-organizer" name="customer_type" value="home-organizer" checked>
  <label for="home-organizer">Home Organizer</label>
</div>
<div>
  <input type="radio" id="school" name="customer_type" value="school">
  <label for="school">School or University</label>
</div>
<div>
  <input type="radio" id="corporate" name="customer_type" value="corporate">
  <label for="corporate">Corporate Buyer</label>
</div>
<div>
  <input type="radio" id="retail" name="customer_type" value="retail">
  <label for="retail">Retail Store</label>
</div>
<div>
  <input type="radio" id="furnishing-dealer" name="customer_type" value="furnishing-dealer">
  <label for="furnishing-dealer">Furnishing Dealer</label>
</div>
<div>
  <input type="radio" id="furnishing-manufacturer" name="customer_type" value="furnishing-manufacturer">
  <label for="furnishing-manufacturer">Furnishing Manufacturer</label>
</div>
<div>
  <input type="radio" id="investor" name="customer_type" value="investor">
  <label for="investor">Investor</label>
</div>
<div>
  <input type="radio" id="media" name="customer_type" value="media">
  <label for="media">Media</label>
</div>
<div>
  <input type="radio" id="other" name="customer_type" value="other">
  <label for="other">Other</label>
</div>
</div>

