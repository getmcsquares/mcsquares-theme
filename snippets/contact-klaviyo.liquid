<script type="text/javascript">
$('input[type=radio][name=customer_type]').change(function() {
    if (this.value == 'home-organizer') {
        $('.swappable-form').hide();
        $('#home-organizer-form').fadeIn();
    }
    else if (this.value == 'school') {
        $('.swappable-form').hide();
        $('#school-form').fadeIn();
    }
    else if (this.value == 'corporate') {
        $('.swappable-form').hide();
        $('#cb-form').fadeIn();
    }
    else if (this.value == 'retail') {
        $('.swappable-form').hide();
        $('#retail-form').fadeIn();
    }
    else if (this.value == 'furnishing-dealer') {
        $('.swappable-form').hide();
        $('#fd-form').fadeIn();
    }
  else if (this.value == 'furnishing-manufacturer') {
        $('.swappable-form').hide();
        $('#fn-form').fadeIn();
    }
    else if (this.value == 'investor') {
        $('.swappable-form').hide();
        $('#investor-form').fadeIn();
    }});
    else if (this.value == 'media') {
        $('.swappable-form').hide();
        $('#media-form').fadeIn();
    }
    else if (this.value == 'other') {
        $('.swappable-form').hide();
        $('#other-form').fadeIn();
    }
</script>